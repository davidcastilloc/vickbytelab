{
    "root": true,
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "@nuxtjs/eslint-config-typescript",
      "plugin:vue/vue3-strongly-recommended"
    ],
    "overrides": [
      {
        "files": [
          "./server/**/*.ts"
        ],
        "rules": {
          "no-console": [
            "error",
            {
              "allow": [
                "info",
                "warn",
                "trace",
                "error"
              ]
            }
          ]
        }
      }
    ]
  }
  